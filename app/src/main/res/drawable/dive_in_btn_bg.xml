<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <shape >
        <gradient android:startColor="#8F636161" android:endColor="#9AD7B5F4" android:angle="45" />
        <corners android:radius="10dp"/>
        <stroke android:width="2dp" android:color="#fafafa"/>
    </shape>
</item>
<item android:state_pressed="true">
    <shape >
        <gradient android:startColor="#8F636161" android:endColor="#9AD7B5F4" android:angle="135" />
        <corners android:radius="10dp"/>
        <stroke android:width="2dp" android:color="#fafafa"/>
    </shape>
</item>
</selector>

